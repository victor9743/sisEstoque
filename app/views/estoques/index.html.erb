<p id="notice"><%= notice %></p>

<h1>Estoques</h1>

<table>
  <thead>
    <tr>
      <th>Qtdentrada</th>
      <th>Qtdsaida</th>
      <th>Dataentrada</th>
      <th>Datasaida</th>
      <th>Valorentrada</th>
      <th>Valorsaida</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estoques.each do |estoque| %>
      <tr>
        <td><%= estoque.qtdentrada %></td>
        <td><%= estoque.qtdsaida %></td>
        <td><%= estoque.dataentrada %></td>
        <td><%= estoque.datasaida %></td>
        <td><%= estoque.valorentrada %></td>
        <td><%= estoque.valorsaida %></td>
        <td><%= link_to 'Show', estoque %></td>
        <td><%= link_to 'Edit', edit_estoque_path(estoque) %></td>
        <td><%= link_to 'Destroy', estoque, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Estoque', new_estoque_path %>
